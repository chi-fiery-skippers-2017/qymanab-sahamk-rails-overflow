<!DOCTYPE html>
<html>
  <head>
    <title>OverflowClone</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div id="headers">
      <button class="button"><%= link_to 'Home', root_path %></button>
      <button class="button"><%= link_to 'New Question', new_question_path %></button>
    </div>
    <%= yield %>
  </body>
</html>
